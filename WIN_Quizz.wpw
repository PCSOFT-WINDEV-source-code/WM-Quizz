      // Summary: Was the question already asked?
      // Parameters:
      //	nIDQuestion: ID of the question that we want to ask
      // Return value:
      // 	Undefined type: True if this question was already asked
      //
      PROCEDURE QuestionAlreadyAsked(nQuestionID)
      
      IF ArrayCount(garrQuestionsAlreadyAsked) = gnNbQuestions THEN
      	RESULT -1
      END
      
      IF ArraySeek(garrQuestionsAlreadyAsked,asLinear,nQuestionID) <> -1 THEN
      	RESULT True
      ELSE
      	RESULT False
      END
     type : 458752
   -
     name : DisplayQuestion
     procedure_id : 1652781936131862186
     type_code : 14
     code : |1-
      // Summary: Displays the question of the current record
      // Parameters:
      //	nQuestionID: ID of the question for the current record
      //	gnQuestionLevel: Level of the current question
      //	sText (ANSI string): Text of the current question
      //	nAnswerType (integer): Type of answer, TYP_QCM or TYPE_EDIT
      // Return value:
      // 	None
      //
      PROCEDURE DisplayQuestion(LOCAL nQuestionID, LOCAL nQuestionLevel, LOCAL sText is string, LOCAL nAnswerType is int)
      
      // Content of the question
      sQuestion is string = sText
      IF  StringCount(sQuestion,"?") = 0 THEN
      	sQuestion += " ?"
      END
      
      // Replace " ?" by a on-breakable space to avoid the line wraps just with a "?"
      sQuestion = Replace(sQuestion," ?",Charact(0160)+"?") 
      
      STC_Question = sQuestion
      
      
      // display
      Visible_GR_CorrectAnswer(False)
      
      FOR i = 1 _TO_ NB_ANSWER
      	{"BTN_ANSWER" + i, indControl}..BrushColor = IMG_Answer_normal..BrushColor
      END
      
      // Number of the question
      gnCurrentQuestion++
      STC_Question_Num = gnCurrentQuestion + "/10"
      
      
      // Add into the array
      ArrayAdd(garrQuestionsAlreadyAsked,nQuestionID)
      
      // Possible answers
      IF nAnswerType = TYPE_QCM  THEN
      	// Fill the radio button with the answers
      	FillRadioButton(nQuestionID,nQuestionLevel)
      END
      
      Visible_GR_CorrectAnswer(False)
      
      // Start the stopwatch
      ChronoStart(1)
      // Stress mode
      IF gbStressMode THEN
      	StartStopwatch()
      END
     type : 458752
   -
     name : FillRadioButton
     procedure_id : 1652781936131927722
     type_code : 14
     code : |1+
      // Summary: Fills the radio button for the answers
      // Parameters:
      //	nQuestionID: ID of the question
      //	gnQuestionLevel: Level of the question
      // Return value:
      // 	None
      //
      PROCEDURE FillRadioButton(LOCAL nQuestionID, LOCAL nQuestionLevel)
      
      // Local variables
      nSubscript is int = 1
      
      // hides the last one in case there is 2 only
      BTN_ANSWER3..Visible = False
      
      // Read the responses
      HReadSeekFirst(Answer,QuestionID,nQuestionID)
      WHILE HFound(Answer)
      	// Up to 3 answers
      	IF nSubscript > NB_ANSWER THEN
      		BREAK
      	END
      	
      	IF nQuestionLevel <> LEV_MIX _OR_ Answer.Level = gnLevel THEN
      		IF nSubscript = 3 THEN
      			BTN_ANSWER3..Visible = True
      		END
      		
      		// Assignment
      		{"BTN_ANSWER" + nSubscript,indControl}..Caption = Answer.Text
      		
      		// Correct answer
      		IF Answer.Correct THEN
      			gsCorrectAnswer = Answer.Text
      			gnSubscriptCorrectAnswer = nSubscript
      		END
      		
      		// Next answer
      		nSubscript++
      	END
      	
      	
      	HReadNext(Answer)
      END
      
      
      
      
      
      
      
      
      
      
      
      
      
      
      
      
