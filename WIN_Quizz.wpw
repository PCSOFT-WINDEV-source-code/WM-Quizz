      gnNbCorrectAnswers			is int = 0
      gnTypeCurrentQuestion		is int
      gnSubscriptCorrectAnswer		is int
      gnTotalTimePassed			is int
      gbActive 					is boolean = True
      
      
      // Start the quizz
      StartQuizz()
      
      
