     name : GenerateQuestion
     internal_properties : CAAAAAgAAAAOqWoJyOTS4drqF/UqhusS5yRE7LpDpb6QarQFZdw6q0bTcRRBsgAtkPgqGA5m1d6cLOw1xl510xjk7V7UxHy9V6j60mxqn7pWHjrgQvPcT8zRv/dTNNnmst96dMkq9Jm5aI/FOk8bxPYhnPsaFhf640FFLNZDBh3G85KMS9e1aFSu6FxVtBsG4s9R10pSa2WDTaNatd4=
     procedure_id : 1652781936131731114
     type_code : 14
     code : |1+
      // Summary: Displays a question at random
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      
      PROCEDURE GenerateQuestion()
      
      // End of quizz
      IF gnCurrentQuestion >= NB_QUESTION THEN
      	EndQuizz()
      	RETURN
      END
      
      // Local variables
      nRandom is int
      nSubscript is int = 1
      
      // Creation of random
      nRandom = Random(1,gnNbQuestions)
      
      // Browse the questions
      HReadFirst(Question,gsSearchKey)
      WHILE NOT HOut()
      	IF NOT gbStressMode _OR_ Question.AnswerType<>TYPE_EDIT THEN
      		// Is it the requested question
      		IF nSubscript = nRandom THEN
      			// Was it already asked?
      			SWITCH QuestionAlreadyAsked(Question.QuestionID)
      				CASE True
      					// Initialize the random
      					InitRandom()
      					
      					// Call this same method
      					GenerateQuestion()
      				CASE False
      					// Explanation of the answer
      					gsExplanation = Question.Explanation
      					gnTypeCurrentQuestion = Question.AnswerType
      					gsCorrectAnswer = ""
      					gnSubscriptCorrectAnswer = 0
      					
      					// Display the question
      					DisplayQuestion(Question.QuestionID,Question.Level,Question.Text,Question.AnswerType)
      				OTHER CASE
      					EndQuizz()
      			END
      			
      			BREAK
      		END
      		
      		// Go to next record
      		nSubscript++
      	END
      	
      	HReadNext(Question,gsSearchKey)
      END
      
      
      
      
      
      
      
      
      
      
