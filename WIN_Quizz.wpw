      // Summary: Starts a quiz
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      PROCEDURE StartQuizz()
      
      // Stress mode enabled
      IF gbStressMode=True THEN
      	PROGBAR_Time..Visible  = True
      END
      
      gnNbQuestions = 0
      
      // Initialize the random
      InitRandom()
      
      // Filter on the level of question
      gsSearchKey = HFilter(Question,Level,gnLevel)
      
      // Retrieve the number of questions for this level
      HReadFirst(Question,gsSearchKey)
      
      WHILE HOut(Question)=False
      	IF NOT gbStressMode _OR_ Question.AnswerType <> TYPE_EDIT THEN
      		gnNbQuestions ++	
      	END
      	
      	HReadNext(Question,gsSearchKey)
      END
      
      // By default, no answer was given to the 10 questions yet
      FOR i =1 TO 10
      	{"IMG_Question" + i, indControl} = IMG_Answer_normal..BrushColor
      END
      
      // Generate a question
      IF gnNbQuestions > 0 THEN
      	// Displays a question
      	GenerateQuestion()
      ELSE
      	Error("No question available for this mode.")
      	Close()
      END
