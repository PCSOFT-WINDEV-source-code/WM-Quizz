      PROCEDURE SaveScore()
      
      // Level
      Score.Level = gnLevel
      
      // calculate the score
      IF gnNbCorrectAnswers > NB_QUESTION THEN
      	gnNbCorrectAnswers = NB_QUESTION
      END
      Score.Value = gnNbCorrectAnswers
      
      Score.Time = gnTotalTimePassed
      
      // Date time
      dtNow is datetime
      Score.DateTime = dtNow
      
      // Add the score
      IF HAdd(Score) THEN
      	
      ELSE
      	Error(HErrorInfo() )
      END
