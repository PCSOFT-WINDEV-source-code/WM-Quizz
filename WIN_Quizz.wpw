   -
     name : ValidateAnswer
     procedure_id : 1652781936131993258
     type_code : 14
     code : |1+
      // Summary: Procedure called when pressing a button
      // Parameters:
      //	sAnswer: Text of the answer validated by the user
      //	nAnswerSubscript: Subscript of the answer validated by the user (included between 1 and 3)
      // Return value:
      // 	None
      //
      PROCEDURE ValidateAnswer(sAnswer, nAnswerSubscript)
      
      IF gbActive THEN
      	bCorrectAnswer is boolean = False
      	
      	// end of stopwatch
      	gnTotalTimePassed += ChronoEnd(1)
      	
      	// stop the stopwatch if we are in stress mode
      	IF gbStressMode THEN
      		// Stop the stopwatch
      		StopStopwatch()
      	END
      	
      	IF StringCompare(gsCorrectAnswer,sAnswer) = 0 THEN
      		// It is the correct answer
      		bCorrectAnswer = True
      		gnNbCorrectAnswers ++
      	END
      	
      	IF  NOT gbStressMode THEN
      		
      		DisplayCorrectAnswer(bCorrectAnswer,nAnswerSubscript)
      		
      	ELSE
      		// Display the correct answer
      		DisplayCorrectAnswer(bCorrectAnswer,nAnswerSubscript,True)
      		
      		// wait during 1 second
      		<COMPILE IF ConfigurationType=Android OR ConfigurationType=iOS>
      			Multitask(-100)
      		<END>
      		<COMPILE IF ConfigurationType=UniversalWindowsApp>
      			ThreadPause(100)
      		<END>
      		
      		
      		// Enables the buttons
      		FOR i = 1 _TO_ NB_ANSWER
      			{"BTN_ANSWER" + i, indControl}..State = Active
      		END
      		
      		// Re-enable the program
      		gbActive = True
      		
      		PROGBAR_Time..Visible=False
      		
      		// Continue
      		GenerateQuestion()
      	END
      END
      
