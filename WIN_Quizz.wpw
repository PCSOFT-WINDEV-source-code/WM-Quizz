     name : DisplayCorrectAnswer
     procedure_id : 1652781936132124330
     type_code : 14
     code : |1-
      // Summary: Displays the answers on the user screen
      // Syntax:
      //DisplayCorrectAnswer (<bCorrectlyAnswered>, <nAnswerSubscript> [, <bStress> is boolean])
      //
      // Parameters:
      //	bCorrectlyAnswered:True if the answer to the question is correct
      //	nAnswerSubscript:Subscript of the user answer (included between 1, 2 and 3
      //	bStress (boolean - default value=0):Indicates whether we are in stress mode or not
      
      // Return value:
      PROCEDURE DisplayCorrectAnswer(bCorrectlyAnswered,nAnswerSubscript,bStress is boolean=False)
      
      gbActive = False
      FOR i = 1 _TO_ NB_ANSWER
      	{"BTN_ANSWER" + i, indControl}..BrushColor = IMG_Answer_normal..BrushColor
      	<COMPILE IF ConfigurationType=Android OR ConfigurationType=iOS>
      		{"BTN_ANSWER" + i, indControl}..State = Inactive
      	<END>
      END
      
      
      IF gnTypeCurrentQuestion = TYPE_QCM THEN
      	// MCQ
      	IF bStress = False THEN
      		Visible_GR_CorrectAnswer(True)
      	END
      	
      	IF bCorrectlyAnswered THEN
      		// CORRECT ANSWER
      		IF nAnswerSubscript > 0 THEN
      			{"BTN_ANSWER" + nAnswerSubscript, indControl}..BrushColor = IMG_Correct_answer..BrushColor
      			{"IMG_Question" + gnCurrentQuestion, indControl}..BrushColor = IMG_Correct_answer_top..BrushColor
      		END
      		
      	ELSE
      		// INCORRECT ANSWER
      		IF nAnswerSubscript > 0 THEN
      			{"BTN_ANSWER" + nAnswerSubscript, indControl}..BrushColor = IMG_Incorrect_answer..BrushColor
      		END
      		
      		IF gnSubscriptCorrectAnswer > 0 THEN
      			{"BTN_ANSWER" + gnSubscriptCorrectAnswer, indControl}..BrushColor = IMG_Correct_answer..BrushColor		
      		END
      		{"IMG_Question" + gnCurrentQuestion, indControl}..BrushColor = IMG_Incorrect_answer_top..BrushColor
      	END
      END
      
      // Explanation
      IF Length(gsExplanation) < 2 OR bStress=True THEN
      	STC_Explanation..Visible = False
      ELSE
      	STC_Explanation..Visible = True
      END
      
      STC_Explanation = gsExplanation
