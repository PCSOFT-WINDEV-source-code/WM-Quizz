     code : |1+
      // Summary: Procedure called by TimerSys, used to refresh the stopwatch and to display the correct answer if the time is over in stress mode
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      PROCEDURE RefreshStopwatch()
      
      
      // Progress bar
      PROGBAR_Time =Val(PROGBAR_Time..MaxValue) - Val(ChronoValue(1))
      
      IF PROGBAR_Time<=3000 THEN
      	PROGBAR_Time..Image = IMG_PROGBAR_FIN..BrushColor
      	
      ELSE
      	PROGBAR_Time..Image = IMG_PROGBAR_OK..BrushColor
      END
      
      // Manage the end
      IF PROGBAR_Time = 0 THEN
      	// Validate the answer
      	ValidateAnswer("",0)
      END
      
